{
  "name": "@hanzo/vaul-fork",
  "version": "0.150.1",
  "description": "Hanzo: Drawer component for React.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "pnpm type-check && bunchee",
    "dev": "bunchee --watch",
    "dev:web": "turbo run dev --filter=website...",
    "dev:test": "turbo run dev --filter=test...",
    "format": "prettier --write .",
    "test": "playwright test",
    "clean:nm": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "clean:nxt": "find . -name '.next' -type d -prune -exec rm -rf '{}' +",
    "clean:all": "pnpm clean:nm && pnpm clean:nxt && rm pnpm-lock.yaml"
  },
  "keywords": [
    "react",
    "drawer",
    "dialog",
    "modal"
  ],
  "author": "Emil Kowalski <e@emilkowal.ski>",
  "license": "MIT",
  "homepage": "https://vaul.emilkowal.ski/",
  "repository": {
    "type": "git",
    "url": "https://github.com/hanzoai/vaul-fork.git"
  },
  "bugs": {
    "url": "https://github.com/emilkowalski/vaul/issues"
  },
  "devDependencies": {
    "@playwright/test": "^1.37.1",
    "@types/node": "20.5.7",
    "@types/react": "18.3.2",
    "@types/react-dom": "18.3.0",
    "bunchee": "^4.4.6",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "turbo": "1.6",
    "typescript": "5.3.3"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.4"
  }
}
